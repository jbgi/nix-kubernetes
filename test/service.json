{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "nginx"
  },
  "spec": {
    "selector": {
      "app": "nginx"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 80,
        "targetPort": "80"
      },
      {
        "protocol": "TCP",
        "port": 443,
        "targetPort": "443"
      }
    ]
  }
}
